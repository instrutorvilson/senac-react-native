create table carro(
	id serial primary key,
	placa varchar(8),
	marca varchar(20)
);

insert into carro(placa, marca)values('MEn7456','Toyota');
insert into carro(placa, marca)values('QHZ-1978','Honda');
insert into carro(placa, marca)values('LZZ-4186','Volkswaggen');
insert into carro(marca)values('Volkswaggen');

update carro set
placa = 'ALU-4567',
marca = 'Chevrolet'
where id = 4;

select * from carro;
